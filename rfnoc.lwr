# Initializes a prefix with the necessary configuration to support RFNoC and RFNoC modtool
inherit: prefix 
config:
    packages:
        uhd: 
            gitbranch: rfnoc-devel
            forcebuild: True
        gnuradio:
            forcebuild: True
        gr-ettus:
            gitbranch: master
            forcebuild: True
        uhd-fpga:
            gitbranch: rfnoc-devel
depends:
- gnuradio
- uhd
- gr-ettus
- uhd-fpga
